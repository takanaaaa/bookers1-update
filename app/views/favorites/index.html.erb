<%= render "layouts/profile", user: @user%>
<h2 class="my-3">my favorites</h2>
<div class="container py-5">
  <div class="row justify-content-center">
    <% if @favorite_list.present? %>
      <%= render 'books/lists', books: @favorite_list %>
    <% end %>
  </div>
  <duv class="row justify-content-center">
    <%= paginate @favorites %>
  </duv>
</div>